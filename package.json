{
  "name": "outfitter",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "prepare": "husky",
    "i18n:check": "if command -v bun >/dev/null 2>&1; then bun scripts/checkTranslations.ts; else node scripts/checkTranslations.ts; fi",
    "db:migrate": "if command -v bun >/dev/null 2>&1; then bun scripts/db/migrate.ts; else node scripts/db/migrate.ts; fi",
    "db:create-migration": "if command -v bun >/dev/null 2>&1; then bun scripts/db/createMigration.ts; else node scripts/db/createMigration.ts; fi",
    "android:emulator": "emulator -avd Medium_Phone_API_36.0",
    "android:open": "adb shell am start -a android.intent.action.VIEW -d http://10.0.2.2:5173",
    "android:port-forward": "adb forward tcp:9222 localabstract:chrome_devtools_remote",
    "repo:version-bump": "scripts/version-bump.sh",
    "repo:docker-build": "scripts/build-image.sh",
    "repo:deploy": "scripts/build-image.sh"
  },
  "lint-staged": {
    "*.{js,ts,svelte,json,css,md}": "prettier --write"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/js": "^9.33.0",
    "@internationalized/date": "^3.8.2",
    "@lucide/svelte": "^0.515.0",
    "@sveltejs/enhanced-img": "^0.8.1",
    "@sveltejs/kit": "^2.34.1",
    "@sveltejs/vite-plugin-svelte": "^6.1.3",
    "@tailwindcss/vite": "^4.1.12",
    "@tanstack/table-core": "^8.20.5",
    "@types/bun": "latest",
    "bits-ui": "^2.9.4",
    "embla-carousel-svelte": "^8.6.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.11.0",
    "formsnap": "^2.0.1",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "layerchart": "^1.0.11",
    "lint-staged": "^16.1.5",
    "paneforge": "^1.0.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "svelte": "^5.38.2",
    "svelte-check": "^4.3.1",
    "tailwind-variants": "^3.1.1",
    "tailwindcss": "^4.1.12",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "vite": "^7.1.3"
  },
  "dependencies": {
    "@simplewebauthn/browser": "^13.1.2",
    "@simplewebauthn/server": "^13.1.2",
    "@sveltejs/adapter-node": "^5.3.1",
    "@types/canvas-confetti": "^1.9.0",
    "@types/chalk": "^2.2.4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/nodemailer": "^7.0.0",
    "@types/pg": "^8.15.5",
    "@types/qrcode": "^1.5.5",
    "@types/speakeasy": "^2.0.10",
    "bcryptjs": "^3.0.2",
    "canvas-confetti": "^1.9.3",
    "chalk": "^5.6.0",
    "clsx": "^2.1.1",
    "inquirer": "^12.9.3",
    "jsonwebtoken": "^9.0.2",
    "mode-watcher": "^1.1.0",
    "nodemailer": "^7.0.5",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "redis": "^5.8.2",
    "speakeasy": "^2.0.0",
    "sveltekit-superforms": "^2.27.4",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.17"
  }
}
